<?xml version="1.0" encoding="utf-8"?><Database Name="Database_Task2_PT" Class="BazaDataContext" xmlns="http://schemas.microsoft.com/linqtosql/dbml/2007">
  <Table Name="dbo.Catalogs" Member="Catalogs">
    <Type Name="Catalog">
      <Column Name="Title" Type="System.String" DbType="VarChar(100)" CanBeNull="true" />
      <Column Name="Author" Type="System.String" DbType="VarChar(100)" CanBeNull="true" />
      <Column Name="Id" Type="System.Int32" DbType="Int NOT NULL" IsPrimaryKey="true" CanBeNull="false" />
      <Association Name="Catalog_State" Member="States" ThisKey="Id" OtherKey="BookId" Type="State" />
    </Type>
  </Table>
  <Table Name="dbo.States" Member="States">
